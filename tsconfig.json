{
	"compilerOptions": {
		"target": "es6",
		"lib": ["es5"],
		"module": "commonjs",
		"moduleResolution": "node",
		"declaration": true,
		"outDir": "lib",
		"rootDir": "src",
		"declarationDir": "types",
		"downlevelIteration": true
